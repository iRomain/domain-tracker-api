{
  "name": "domain-tracker-api",
  "version": "0.0.1",
  "description": "Domain tracker used to track my domains across registrars",
  "main": "api.js",
  "scripts": {
    "lab": "lab -C -I '__core-js_shared__,core,CSS'",
    "lint": "standard --verbose | snazzy",
    "fix": "standard --fix",
    "dev-start": "npm run fix && npm run lint && npm run lab && NODE_ENV=dev node api.js",
    "start": "node api.js",
    "test": "npm run lint && npm run lab && lab -r lcov > lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://github.com/iRomain/domain-tracker-api.git"
  },
  "keywords": [
    "domain name",
    "registrar",
    "microservice",
    "hapi"
  ],
  "author": "Romain Barissat",
  "license": "MIT",
  "homepage": "https://github.com/iRomain/domain-tracker-api#readme",
  "bugs": "https://github.com/iRomain/domain-tracker-api/issues",
  "engines": {
    "node": "latest"
  },
  "dependencies": {
    "@hapi/good": "latest",
    "@hapi/good-console": "latest",
    "@hapi/hapi": "latest",
    "hapi-swagger": "latest",
    "@hapi/joi": "latest",
    "@hapi/inert": "latest",
    "@hapi/vision": "latest",
    "newrelic": "latest",
    "puppeteer": "^1.17.0"
  },
  "devDependencies": {
    "@hapi/code": "latest",
    "@hapi/lab": "latest",
    "colors": "latest",
    "nodemon": "latest",
    "snazzy": "^8.0.0",
    "standard": "latest"
  }
}
